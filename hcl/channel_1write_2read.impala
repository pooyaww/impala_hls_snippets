static mut ch : channel_i32;

fn main() -> () {
    let device = 0;
    let ptr = alloc_hls(device, sizeof[i32]() * 4i64).data as &mut [i32];
    with hls(device) {
        write_channel_i32(&mut ch, ptr(0));
    }

    with hls(device) {
        ptr(0) = read_channel_i32(&mut ch);
    }

    with hls(device) {
        ptr(0) = read_channel_i32(&mut ch);
    }
}
